  <%= csrf_meta_tags %>
  <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
  <script type="text/javascript">
   function render_content(category){
			var frame = $("#frame_content"), protocol = document.location.protocol, host = document.location.host, pathname = document.location.pathname;
			var window_url = protocol + "//" + host + pathname;
			if(category == "guests")
				frame.attr('src', window_url+'/attendees');
			else
				frame.attr('src',pathname+'/supplemental_info');
		}
</script>
</head>
